<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html b:css='false' b:defaultwidgetversion='2' b:layoutsVersion='3' b:responsive='true' b:templateVersion='1.0.0' expr:lang='data:blog.locale' expr:class='data:blog.languageDirection' expr:dir='data:blog.languageDirection' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>

  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no" />
    <b:include name='head' />

    <b:comment>=== HEAD-CSS ===</b:comment>
    <b:if cond='!data:view.isLayoutMode'>
      <style type="text/css">
        /* <![CDATA[ */
        html,
        body {
          overflow-x: hidden;
        }

        body {
          position: relative;
        }

        /* code css here */
        /* ]]> */
      </style>
    </b:if>

    <b:comment>=== B-SKIN ===</b:comment>
    <b:if cond='!data:view.isLayoutMode'>
      <b:skin>
      <![CDATA[
      /* code css here */
      ]]>
      </b:skin>
    </b:if>

    <b:comment>=== LAYOUT-MODE ===</b:comment>
    <b:if cond='data:view.isLayoutMode'>
      <b:template-skin>
      <![CDATA[
      /* code css here */
      ]]>
      </b:template-skin>
    </b:if>

    <b:comment>=== MARKUPS ===</b:comment>
    <b:defaultmarkups>
      <b:defaultmarkup type='Common'>
        <b:includable id='head'>
          <title>
            <data:view.title.escaped />
          </title>
          <b:include name='_head-link' />
          <b:include data='blog' name='all-head-content' />
          <b:include data='blog' name='google-analytics' />
        </b:includable>
        <b:includable id='_head-link'>
          <link rel='dns-prefetch' href='//1.bp.blogspot.com' />
          <link rel='dns-prefetch' href='//2.bp.blogspot.com' />
          <link rel='dns-prefetch' href='//3.bp.blogspot.com' />
          <link rel='dns-prefetch' href='//4.bp.blogspot.com' />
          <link rel='dns-prefetch' href='//www.blogger.com' />
          <link rel='dns-prefetch' href='//img1.blogblog.com' />
          <link rel='dns-prefetch' href='//resources.blogblog.com' />
          <link rel='dns-prefetch' href='//lh3.googleusercontent.com' />
          <link rel='dns-prefetch' href='//apis.google.com' />
          <link rel='dns-prefetch' href='//fonts.gstatic.com' />
          <link rel='dns-prefetch' href='//fonts.googleapis.com' />
          <link rel='dns-prefetch' href='//ajax.googleapis.com' />
          <link rel='dns-prefetch' href='//pagead2.googlesyndication.com' />
          <link rel='dns-prefetch' href='//googleads.g.doubleclick.net' />
          <link rel='dns-prefetch' href='//tpc.googlesyndication.com' />
          <link rel='dns-prefetch' href='//www.googletagservices.com' />
          <link rel='dns-prefetch' href='//www.googletagmanager.com' />
          <link rel='dns-prefetch' href='//www.google-analytics.com' />
          <link rel='dns-prefetch' href='//cdn.jsdelivr.net' />
          <link rel='dns-prefetch' href='//cdnjs.cloudflare.com' />
          <link rel='dns-prefetch' href='//www.facebook.com' />
          <link rel='dns-prefetch' href='//connect.facebook.net' />
          <link rel='dns-prefetch' href='//static.xx.fbcdn.net' />
          <link rel='dns-prefetch' href='//c.disquscdn.com' />
          <link rel='dns-prefetch' href='//disqus.com' />
          <link rel='dns-prefetch' href='//twitter.com' />
          <link rel='dns-prefetch' href='//platform.twitter.com' />
          <link rel='dns-prefetch' href='//syndication.twitter.com' />
          <link rel='dns-prefetch' href='//www.pinterest.com' />
          <link rel='dns-prefetch' href='//www.linkedin.com' />
        </b:includable>
        <b:includable id='translate'>
          <b:switch var='data:message'>
            <b:case value='prevPost' />
            <b:if cond='data:blog.locale.language == "en"'><data:messages.newer />
              <b:elseif cond='data:blog.locale.language == "vi"' /><data:messages.newer />
              <b:else /><data:messages.newer />
            </b:if>
            <b:case value='nextPost' />
            <b:if cond='data:blog.locale.language == "en"'><data:messages.older />
              <b:elseif cond='data:blog.locale.language == "vi"' /><data:messages.older />
              <b:else /><data:messages.older />
            </b:if>
            <b:default />
            <data:message />
          </b:switch>
        </b:includable>
      </b:defaultmarkup>
    </b:defaultmarkups>

    <b:comment>=== HEAD-JS ===</b:comment>
    <b:if cond='data:widgets.AdSense.first or data:blog.adsenseClientId'>
      <b:comment>### Google AdSense ###</b:comment>
      <script async='async' src='//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js' />
    </b:if>
    <script>
      /* code js here */
    </script>
  </head>

  <body>
    <b:comment>=== BODY-CLASS ===</b:comment>
    <b:class cond='data:view.isHomepage' name='is-homepage' />
    <b:class cond='data:view.isPost' name='is-post' />
    <b:class cond='data:view.isPage' name='is-page' />
    <b:class cond='data:view.isLabelSearch' name='is-search-label' />
    <b:class cond='data:view.search.query' name='is-search-query' />
    <b:class cond='data:view.isSearch and !data:view.isLabelSearch and !data:view.search.query' name='is-search-default' />
    <b:class cond='data:view.isArchive' name='is-archive' />
    <b:class cond='data:view.isError' name='is-error' />
    <b:class cond='data:view.isPreview' name='is-preview' />

    <b:comment>=== HEADER ===</b:comment>
    <header>
      <h1>
        <a expr:href='data:blog.homepageUrl'>
          <data:blog.title />
        </a>
      </h1>
    </header>

    <b:comment>=== CONTENT ===</b:comment>
    <main>
      <b:section class='main-section' id='main-section' maxwidgets='1' name='Main section' showaddelement='no'>
        <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog' visible='true'>
          <b:includable id='main' var='this'>

            <b:comment>=== is-Post ===</b:comment>
            <b:if cond='data:posts.notEmpty'>
              <b:if cond='data:view.isPost'>
                <b:comment>=== URL: https://blogName.blogspot.com/{year}/{month}/{post-link}.html ===</b:comment>
                <b:comment>=== Specific post url ===</b:comment>
                <b:if cond='data:view.url == "https://blogName.blogspot.com/{year}/{month}/{post-link}.html"'>
                </b:if>
                <b:loop values='data:posts' var='post'>
                  <article expr:id='"post-" + data:post.id'>
                    <b:comment>=== Title ===</b:comment>
                    <h1>
                      <b:eval expr='data:post.title ? data:post.title : data:messages.noTitle' />
                    </h1>
                    <b:comment>=== Body ===</b:comment>
                    <data:post.body />
                  </article>
                </b:loop>
              </b:if>
            </b:if>

            <b:comment>=== is-Page ===</b:comment>
            <b:if cond='data:posts.notEmpty'>
              <b:if cond='data:view.isPage'>
                <b:comment>=== URL: https://blogName.blogspot.com/p/{page-link}.html ===</b:comment>
                <b:comment>=== Specific static page url ===</b:comment>
                <b:if cond='data:view.url == "https://blogName.blogspot.com/p/{page-link}.html"'>
                </b:if>
                <b:loop values='data:posts' var='post'>
                  <article expr:id='"post-" + data:post.id'>
                    <b:comment>=== Title ===</b:comment>
                    <h1>
                      <b:eval expr='data:post.title ? data:post.title : data:messages.noTitle' />
                    </h1>
                    <b:comment>=== Body ===</b:comment>
                    <data:post.body />
                  </article>
                </b:loop>
              </b:if>
            </b:if>

            <b:comment>=== is-error ===</b:comment>
            <b:if cond='data:posts.notEmpty'>
              <b:if cond='data:view.isError'>
                <b:comment>=== URL: https://blogName.blogspot.com/{404} ===</b:comment>
              </b:if>
            </b:if>

            <b:comment>=== is-preview ===</b:comment>
            <b:if cond='data:posts.notEmpty'>
              <b:if cond='data:view.isPreview'>
                <b:comment>=== URL: https://www.blogger.com/blog/post/edit/preview/{blogID}/{postID} ===</b:comment>
              </b:if>
            </b:if>

            <b:comment>=== is-layout-mode ===</b:comment>
            <b:if cond='data:posts.notEmpty'>
              <b:if cond='data:view.isLayoutMode'>
                <b:comment>=== URL: https://www.blogger.com/blog/layout/{blogID} ===</b:comment>
              </b:if>
            </b:if>

            <b:comment>=== is-Homepage | is-search-(label|query|default) | is-archive ===</b:comment>
            <b:if cond='data:posts.notEmpty'>
              <b:if cond='data:view.isMultipleItems'>

                <b:comment>=== is-Homepage ===</b:comment>
                <b:if cond='data:view.isHomepage'>
                  <b:comment>=== URL: https://blogName.blogspot.com ===</b:comment>
                </b:if>

                <b:comment>=== is-search-label ===</b:comment>
                <b:if cond='data:view.isLabelSearch'>
                  <b:comment>=== 1. URL: https://blogName.blogspot.com/search/label/{label-name} ===</b:comment>
                  <b:comment>=== 2. URL: https://blogName.blogspot.com/search?label={label-name} ===</b:comment>
                  <b:comment>=== Specific search label name ===</b:comment>
                  <b:if cond='data:view.search.label == "{label-name}"'>
                  </b:if>
                </b:if>

                <b:comment>=== is-search-query ===</b:comment>
                <b:if cond='data:view.search.query'>
                  <b:comment>=== URL: https://blogName.blogspot.com/search?q={query-value} ===</b:comment>
                  <b:comment>=== Specific search query value ===</b:comment>
                  <b:if cond='data:view.search.query == "{query-value}"'>
                  </b:if>
                </b:if>

                <b:comment>=== is-search-default ===</b:comment>
                <b:if cond='data:view.isSearch and !data:view.isLabelSearch and !data:view.search.query'>
                  <b:comment>=== URL: https://blogName.blogspot.com/search ===</b:comment>
                </b:if>

                <b:comment>=== is-archive ===</b:comment>
                <b:if cond='data:view.isArchive'>
                  <b:comment>=== 1. URL: https://blogName.blogspot.com/{year} ===</b:comment>
                  <b:comment>=== 2. URL: https://blogName.blogspot.com/{year}/{month} ===</b:comment>
                  <b:comment>=== 3. URL: https://blogName.blogspot.com/{year}_{month}_{day}_archive.html ===</b:comment>
                </b:if>

                <b:loop values='data:posts' var='post'>
                  <article expr:id='"post-" + data:post.id'>
                    <b:comment>=== Title ===</b:comment>
                    <h2>
                      <a expr:href='data:post.link ?: data:post.url'>
                        <b:eval expr='data:post.title ? data:post.title : data:messages.noTitle' />
                      </a>
                    </h2>
                  </article>
                </b:loop>

              </b:if>
            </b:if>

            <b:comment>=== post-Pagination ===</b:comment>
            <b:if cond='data:posts.notEmpty and !data:view.isPage'>
              <nav>
                <a>
                  <b:attr expr:value='data:newerPageUrl ? data:newerPageUrl : "#"' name='href' />
                  <b:class cond='!data:newerPageUrl' name='disabled' />
                  <data:messages.newer />
                </a>
                <a>
                  <b:attr expr:value='data:blog.homepageUrl' name='href' />
                  <data:messages.home />
                </a>
                <a>
                  <b:attr expr:value='data:olderPageUrl ? data:olderPageUrl : "#"' name='href' />
                  <b:class cond='!data:olderPageUrl' name='disabled' />
                  <data:messages.older />
                </a>
              </nav>
            </b:if>

          </b:includable>

          <!-- REMOVE DEFAULT INCLUDABLE -->
          <b:includable id='aboutPostAuthor' />
          <b:includable id='addComments' />
          <b:includable id='commentAuthorAvatar' />
          <b:includable id='commentDeleteIcon' />
          <b:includable id='commentForm' />
          <b:includable id='commentFormIframeSrc' />
          <b:includable id='commentItem' />
          <b:includable id='commentList' />
          <b:includable id='commentPicker' />
          <b:includable id='comments' />
          <b:includable id='commentsTitle' />
          <b:includable id='feedLinks' />
          <b:includable id='feedLinksBody' />
          <b:includable id='homePageLink' />
          <b:includable id='iframeComments' />
          <b:includable id='inlineAd' />
          <b:includable id='nextPageLink' />
          <b:includable id='post' />
          <b:includable id='postBody' />
          <b:includable id='postBodySnippet' />
          <b:includable id='postCommentsAndAd' />
          <b:includable id='postCommentsLink' />
          <b:includable id='postFooter' />
          <b:includable id='postFooterAuthorProfile' />
          <b:includable id='postHeader' />
          <b:includable id='postMeta' />
          <b:includable id='postPagination' />
          <b:includable id='postTitle' />
          <b:includable id='previousPageLink' />
          <b:includable id='threadedCommentForm' />
          <b:includable id='threadedCommentJs' />
          <b:includable id='threadedComments' />
        </b:widget>
      </b:section>
    </main>

    <b:comment>=== FOOTER ===</b:comment>
    <footer>
      <p>FOOTER</p>
    </footer>

    <b:comment>=== BODY-JS ===</b:comment>
    <script type="text/javascript">
      //<![CDATA[
      /* code js here */
      //]]>
    </script>

    <b:comment>=== FAKE-SECTION ===</b:comment>
    <b:comment>- Blogger theme requires at least one `b:section` with `showaddelement='yes'`.</b:comment>
    <b:comment>- Note: the default value of `showaddelement` is `yes`.</b:comment>
    <b:if cond='!data:view.isLayoutMode and (data:blog.view == "NeverAccessThisView")'>
      <b:section id='fake-section' showaddelement='yes' />
    </b:if>

  </body>

</html>